<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="sgmpro"
                   namespace="sgmpro.dominio.configuracion">
  <class name="TipoConvenio">
    <id name="Id" column="tcn_id"><generator class="assigned"/></id>
    <property name="Nombre" column="tcn_nombre" not-null="true"/>
    <property name="Descripcion" column="tcn_descripcion" not-null="true"/>
    <property name="MinCuotas" column="tcn_mincuotas" not-null="true"/>
    <property name="MaxCuotas" column="tcn_maxcuotas" not-null="true"/>
    <property name="MaxCuotasCaidas" column="tcn_maxcuotascaidas" not-null="true"/>
    <property name="ValorMinimoAnticipo" column="tcn_valorminimoanticipo" not-null="true"/>
    <property name="ValorMinimoCuota" column="tcn_valorminimocuota" not-null="true"/>
    <property name="MaxQuita" column="tcn_maxquita" not-null="true"/>
    <property name="Activo" column="tcn_activo" not-null="true"/>
    <property name="TasaPura" column="tcn_tasapura" not-null="true"/>
    <property name="IVAsobreTP" column="tcn_ivasobretp" not-null="true"/>
    <property name="Punitorio" column="tcn_punitorio" not-null="true"/>
    <property name="Honorarios" column="tcn_honorarios" not-null="true"/>
    <property name="ReglaHA" column="tcn_reglaha" not-null="true"/>
    <property name="FormulaBaseConvenio" column="tcn_formulabaseconvenio" not-null="true"/>
    <property name="FormulaCuota" column="tcn_formulacuota" not-null="true"/>
    <property name="FormulaInteres" column="tcn_formulainteres" not-null="true"/>
    <property name="FormulaHonorarios" column="tcn_formulahonorarios" not-null="true"/>
    <property name="FormulaGastos" column="tcn_formulagastos" not-null="true"/>
    <property name="FechaBaja" column="tcn_fechabaja"/>
  </class>
</hibernate-mapping>
